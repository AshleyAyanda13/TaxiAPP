@{
    ViewData["Tittle"] = "TaxiDetailes";
}
@model Taxi
<h4>Taxi Details</h4>
<div class="accordion accordion-flush" id="accordionFlushExample">
    <div class="accordion-item">
        <h2 class="accordion-header" id="flush-headingOne">
            <button class="accordion-button collapsed" type="button" data-bs-toggle="collapse" data-bs-target="#flush-collapseOne" aria-expanded="false" aria-controls="flush-collapseOne">
                Taxi Details
            </button>
        </h2>
        <div id="flush-collapseOne" class="accordion-collapse collapse" aria-labelledby="flush-headingOne" data-bs-parent="#accordionFlushExample">
            <div class="accordion-body">
                <!--Taxi Details-->
                <dl class="row">
                    <dt class=" col-4">Brand Name</dt>
                    <dd class="col-6">@Model.BrandName</dd>
                </dl>
                <dl class="row">
                    <dt class=" col-4">License Plate</dt>
                    <dd class="col-6">@Model.LicensePlate</dd>
                </dl>
                <dl class="row">
                    <dt class=" col-4">Taxi Type</dt>
                    <dd class="col-6">@Model.Type</dd>
                </dl>
                <dl class="row">
                    <dt class=" col-4">Seats</dt>
                    <dd class="col-6">@Model.Seats</dd>
                </dl>
            </div>
        </div>
 <a asp-action="TaxIndex" class="btn btn-success">Back To List</a>
